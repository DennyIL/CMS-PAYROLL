<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="false">

  <class name="BRIChannelSchedulerNew.Payroll.Pocos.UserMap, BRIChannelSchedulerNew" table="usermaps">

    <!-- A 32 hex character is our surrogate key. It's automatically
            generated by NHibernate with the UUID pattern. -->
    <id name="Id">
      <column name="ID" sql-type="Int32" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="ClientHandle">
      <column name="CLIENTHANDLE" length="45" not-null="true" />
    </property>
    <property name="UserHandle">
      <column name="USERHANDLE" length="45" not-null="true" />
    </property>
    <property name="ClientEntity">
      <column name="CLIENTENTITY" not-null="true" />
    </property>
    <property name="UserEntity">
      <column name="USERENTITY" not-null="true" />
    </property>
    <property name="AuthorityEntity">
      <column name="AUTHORITYENTITY" />
    </property>

    <many-to-one name="User"
        column="USERENTITY"
        unique="true"
        not-null="true" update="false" insert="false"/>
    <many-to-one name="UserMatrix"
        column="USERENTITY"
        unique="true"
        not-null="true" update="false" insert="false"/>
    <many-to-one name="Client"
        column="CLIENTENTITY"
        unique="true"
        not-null="true" update="false" insert="false"/>
    <many-to-one name="ClientWorkflow"
        column="CLIENTENTITY"
        unique="true"
        not-null="true" update="false" insert="false"/>
    <many-to-one name="ClientMatrix"
        column="CLIENTENTITY"
        unique="true"
        not-null="true" update="false" insert="false"/>
  </class>

</hibernate-mapping>