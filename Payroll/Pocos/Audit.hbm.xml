<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="false">

  <class name="BRIChannelSchedulerNew.Payroll.Pocos.Audit, BRIChannelSchedulerNew" table="audits">

    <id name="Id">
      <column name="ID" sql-type="Int32" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="User">
      <column name="USER" />
    </property>
    <property name="TransactionId">
      <column name="TRANSACTIONID" />
    </property>
    <property name="Description">
      <column name="DESCRIPTION" />
    </property>
    <property name="Data">
      <column name="DATA" />
    </property>
    <property name="Time">
      <column name="TIME" />
    </property>
    <property name="IPInfo">
      <column name="IPINFO" />
    </property>
  </class>

</hibernate-mapping>