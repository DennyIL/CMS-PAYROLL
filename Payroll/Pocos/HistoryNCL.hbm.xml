<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="false">
	<class name="BRIChannelSchedulerNew.Payroll.Pocos.HistoryNCL, BRIChannelSchedulerNew" table="historyncl">

		<id name="Id">
			<column name="ID" sql-type="Int32" not-null="true"/>
			<generator class="native" />
		</id>

		<property name="ClientId">
			<column name="CLIENTID" />
		</property>

		<property name="ApplicantName">
			<column name="APPLICANTNAME" />
		</property>

    <property name="BeneficiaryName">
      <column name="BENEFICIARYNAME" />
    </property>

		<property name="Description">
			<column name="DESCRIPTION" />
		</property>

		<property name="Type">
			<column name="TYPE" />
		</property>

		<property name="CreatedTime">
			<column name="CREATEDTIME" />
		</property>

		<property name="Currency">
			<column name="CURRENCY" />
		</property>

		<property name="Amount">
			<column name="AMOUNT" />
		</property>

		<property name="DebetKredit">
			<column name="DEBETKREDIT" />
		</property>

		<property name="Status">
			<column name="STATUS" />
		</property>
		
		<property name="ConvAmount">
			<column name="CONVAMOUNT" />
		</property>

    <property name="Kurs">
      <column name="KURS" />
    </property>

    <property name="IssueDate">
      <column name="ISSUEDATE" />
    </property>

    <property name="ExpiryDate">
      <column name="ExpiryDate" />
    </property>

	</class>
</hibernate-mapping>
